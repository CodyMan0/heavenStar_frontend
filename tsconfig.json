{
  "compilerOptions": {
    "allowJs": true,
    "target": "ESNext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "outDir": "./build",
    "moduleResolution": "Node",
    "baseUrl": "src",
    "noImplicitAny": true, 
    "noEmit": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "build"]
}
